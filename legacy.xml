<?xml version="1.0" encoding="UTF-8"?>
<!-- Andromadus modifications to CM build tree -->
<manifest>
  <!-- legacy repositories part. These are incompatible with upstream. -->

  <!-- qcom_display is buggy and deprecated. Our devices are getting old. -->
  <remove-project name="CyanogenMod/android_hardware_qcom_display" />
  <project path="hardware/qcom/display" name="Andromadus/android_hardware_qcom_display" remote="github" revision="cm10.2" />

  <!-- GPS library got lost somewhere when moving from cm10.0 to cm10.1. Let's use the older one. -->
  <remove-project name="CyanogenMod/android_hardware_qcom_gps" />
  <project path="hardware/qcom/gps" name="CyanogenMod/android_hardware_qcom_gps" remote="github" revision="jellybean" />

  <!-- qcom_media should have our "legacy" patches and hacks. -->
  <remove-project name="CyanogenMod/android_hardware_qcom_media" />
  <project path="hardware/qcom/media" name="Andromadus/android_hardware_qcom_media" remote="github" revision="cm10.2" />
</manifest>
